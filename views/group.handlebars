<div class="col-xs-offset-2 col-xs-8">
  <div class="row">
    <h2>
			<span>Group: </span>
			<span>{{group.name}}</span>
		</h2>
  </div>
	{{#if auth}}
		<div class="row">
		  <a href="/group/join/{{group.name}}">Join Group</a>
		</div>
	{{/if}}
	<div class="row">
	  <h3>Subscriptions:</h3>
	</div>
	<div class="row">
		<ul class="list-group">
			{{#each groupSubList}}
				<li class="list-group-item">
						{{> userlink this.user}}
				</li>
			{{/each}}
		</ul>
	</div>
</div>
