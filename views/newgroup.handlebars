<div class="col-xs-offset-2 col-xs-8">
	<div class="row">
		<form name="newGroup" action="/test/newgroup" method="post" onsubmit="return validate()">
			<h3>New Group</h3>
			<div class="form-group">
			  <label for="groupname"></label>
			  <input type="text" name="groupname" class="form-control" id="" placeholder="Group Name">
			</div>
			<input type="submit" name="submit" value="Create Group">
	  </form>
	</div>
	<div class="row">
	  <h3>Your Groups:</h3>
		{{#each group}}
			
		{{/each}}
	</div>
</div>
